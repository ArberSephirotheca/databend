create table t1(a int);

statement ok
insert into t values(1);

statement ok
insert into t1 values(1),(1);

onlyif mysql
statement query T
explain select * from t left join t1 on t.a = t1.a limit 1,2;

----
Limit: [2], Offset: [1]
└── HashJoin: LEFT OUTER, build keys: [t1.a (#1)], probe keys: [CAST(t.a (#0) AS INT NULL)], join filters: []
    ├── Limit: [2], Offset: [1]
    │   └── Scan: default.default.t, filters: [], Sort: [none], limit: [3]
    └── Limit: [2], Offset: [1]
        └── Scan: default.default.t1, filters: [], Sort: [none], limit: [3]

